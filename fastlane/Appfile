# For more information about the Appfile, see:
#     https://docs.fastlane.tools/advanced/#appfile

app_identifier ENV["FASTLANE_APP_ID"] # Get App/Bundle ID environment variable 
apple_id ENV["FASTLANE_APPLE_ID"] # Get Apple ID email address from environment variable (default: Mr. CI)
team_id ENV["FASTLANE_TEAM_ID"] # Get Developer Portal Team ID from environment variable (default: Upnetix)
itc_team_id ENV["FASTLANE_ITC_TEAM_ID"] # Get Itunes Connect (Apple Store) Team ID from environment variable
